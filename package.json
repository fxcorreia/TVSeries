{
  "name": "react-native-base-project",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "compile-ts": "tsc --noEmit",
    "postinstall": "patch-package",
    "run-android:base:dev": "react-native run-android --variant=baseDevDebug --appId com.tvseries.base.dev",
    "run-android:base:qa": "react-native run-android --variant=baseQaDebug --appId com.tvseries.base.qa",
    "run-android:base:prod": "react-native run-android --variant=baseProdDebug --appId com.tvseries.base",
    "run-ios:base:dev": "react-native run-ios --scheme \"Base DEV\"",
    "run-ios:base:qa": "react-native run-ios --scheme \"Base QA\"",
    "run-ios:base:prod": "react-native run-ios --scheme \"Base PROD\""
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.17.7",
    "@react-native-community/hooks": "2.8.1",
    "@react-native-community/netinfo": "9.3.0",
    "@react-navigation/bottom-tabs": "6.3.2",
    "@react-navigation/drawer": "6.4.3",
    "@react-navigation/native": "6.0.11",
    "@react-navigation/native-stack": "6.7.0",
    "@reduxjs/toolkit": "^1.8.3",
    "apisauce": "^2.1.5",
    "i18next": "^21.8.16",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "react": "17.0.2",
    "react-i18next": "^11.18.3",
    "react-native": "0.68.2",
    "react-native-android-keyboard-adjust": "1.2.0",
    "react-native-biometrics": "^2.2.2",
    "react-native-bootsplash": "4.2.3",
    "react-native-config": "1.4.6",
    "react-native-device-info": "10.0.2",
    "react-native-gesture-handler": "2.5.0",
    "react-native-keychain": "8.1.1",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-localize": "2.2.2",
    "react-native-modal": "^13.0.1",
    "react-native-reanimated": "2.9.1",
    "react-native-rename": "^2.9.0",
    "react-native-safe-area-context": "4.3.1",
    "react-native-screens": "3.15.0",
    "react-native-svg": "^13.1.0",
    "react-native-webview": "11.23.0",
    "react-redux": "^8.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "3.0.0",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.182",
    "@types/react-native": "^0.67.3",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^7.32.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-module-resolver": "^1.4.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.67.0",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
